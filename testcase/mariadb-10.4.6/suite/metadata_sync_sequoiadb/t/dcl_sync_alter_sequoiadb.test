USE sdb1;
DELIMITER $$;
CREATE PROCEDURE dcl_alter()
BEGIN
DECLARE i int;
SET i = 0;
WHILE i < 3 do
IF(i = 0)
THEN
ALTER USER u1;
END IF;
IF(i = 1)
THEN
ALTER USER u2 IDENTIFIED BY '123456';
END IF;
IF(i = 2)
THEN
ALTER USER u3 IDENTIFIED WITH 'mysql_native_password' AS '*6BB4837EB74329105EE4568DDA7DC67ED2CA2AD9';
END IF;
SET i = i + 1;
END WHILE;
END $$
DELIMITER ;$$

